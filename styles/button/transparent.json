{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "title": "Transparent",
    "slug": "transparent",
    "blockTypes": ["core/button"],
    "styles": {
        "color": {
            "background": "transparent",
            "text": "var:custom|color|secondary-dark"
        },
        "border": {
            "color": "currentColor",
            "width": "2px",
            "style": "solid"
        },
        "spacing": {
            "padding": {
                "top": "calc(var(--wp--preset--spacing--30) - 2px)",
                "bottom": "calc(var(--wp--preset--spacing--30) - 2px)"
            }
        }
    }
}
